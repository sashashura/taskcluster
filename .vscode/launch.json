  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch vite",
      "program": "${workspaceFolder}/ui/node_modules/vite/bin/vite.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },

    {
      "name": "vite npm",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "vite:dev"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
  ]
}